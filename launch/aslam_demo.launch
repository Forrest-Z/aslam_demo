<launch>
<include file="$(find turtlebot_gazebo)/launch/turtlebot_world.launch" output = "screen" launch-prefix="xterm -e">
  <arg name="world_file" value="worlds/willowgarage.world" />
</include>
<include file="$(find turtlebot_teleop)/launch/keyboard_teleop.launch" output = "screen" launch-prefix="xterm -e"/>

<node pkg="map_server" type="map_server" name="map_server" args="currmap.yaml" output="screen"/>
<node pkg="rviz" type="rviz" name="rviz" args="-d $(find aslam_demo)/rviz/aslam.rviz" output = "screen"/>

<node pkg="aslam_demo" type="aslam_demo_node" name="aslam_demo_node" output="screen" />

</launch>
